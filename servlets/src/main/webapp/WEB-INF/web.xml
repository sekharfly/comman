<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>


	<servlet>
		<servlet-name>demo</servlet-name>
		<servlet-class>servletsBasic.DemoServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Login</servlet-name>
		<servlet-class>servletsBasic.Login</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Fillters</servlet-name>
		<display-name>Fillters</display-name>
		<description></description>
		<servlet-class>fillters.Fillters</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>demo</servlet-name>
		<url-pattern>/welcome</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Login</servlet-name>
		<url-pattern>/Login</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Fillters</servlet-name>
		<url-pattern>/Fillters</url-pattern>
	</servlet-mapping>
</web-app>
